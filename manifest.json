{
  "manifest_version": 2,

  "name": "GitPrint",
  "description": "Turn code into books.",
  "version": "0.0.0",

  "icons": {
    "128": "icon_128.png"
  },

  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": [
             "js/jquery.min.js",
             "js/jquery-ui.min.js",
             "html/index.js",
             "js/add-button.js",
             "js/content.js",
             "js/files.js",
             "js/sort.js",
             "js/print.js",
             "js/modernizr.custom.79639.js"
            ],
      "css": [
             "css/modal.css",
             "css/main.css",
             "css/print.css"
            ],
      "web_accessible_resources": [
            "images/folder.png",
            "images/octocat_300.png"]
    }
  ]
}
